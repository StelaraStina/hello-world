package com.company;

public class Temperatura {

    public double temperatura;
    public void setTemperatura(double temperaturaZilei) {
        this.temperatura = temperaturaZilei;
    }
    public double getTemperatura(){
        return temperatura;
    }

    public double conversionfromFtoC () {
        double convertedTemperatura = 5/9*(this.getTemperatura()-32);
        return convertedTemperatura;
    }
}
